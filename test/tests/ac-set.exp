#! /usr/bin/env expect --

spawn cdecl --color=never
set timeout 1

expect "cdecl> "
# set options
send "set o\t\r"
expect default { exit 1 } " using"

expect "cdecl> "
# set alt-tokens
send "set a\t; set\r"
expect default { exit 1 } " alt-tokens"

expect "cdecl> "
# set nousing
send "set nou\t; set\r"
expect default { exit 1 } " nousing"

